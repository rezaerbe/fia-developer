<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erbe.fiadeveloper">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.FIADeveloper">

        <activity android:name=".ui.ProfileActivity" />
        <activity android:name=".ui.consultation.ListConsultationActivity" />
        <activity android:name=".ui.coaching.ListCoachingActivity" />
        <activity android:name=".ui.original.DetailOriginalActivity" />
        <activity android:name=".ui.original.ListOriginalActivity" />
        <activity android:name=".ui.original.CategoryOriginalActivity" />
        <activity android:name=".ui.video.DetailVideoActivity" />
        <activity android:name=".ui.video.ListVideoActivity" />
        <activity android:name=".ui.article.DetailArticleActivity" />
        <activity android:name=".ui.article.ListArticleActivity" />
        <activity android:name=".ui.article.CategoryArticleActivity" />
        <activity android:name=".ui.coaching.CoachingActivity" />
        <activity android:name=".ui.coaching.DetailCoachActivity" />
        <activity android:name=".ui.coaching.ListCoachActivity" />
        <activity android:name=".ui.consultation.ConsultationActivity" />
        <activity android:name=".ui.consultation.DetailConsultantActivity" />
        <activity android:name=".ui.consultation.ListConsultantActivity" />
        <activity android:name=".ui.ReportActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>